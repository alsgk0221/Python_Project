'''
# 11726

N = 9
result = 0
temp = [1, 2]

i = 2
while len(temp) != N-1:
    temp.append(temp[i-1] + temp[i-2])
    i += 1

result = temp[len(temp)-1] + temp[len(temp)-2]
print(result % 10007)
