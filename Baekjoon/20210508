'''
# 11727

N = 12
result = 0
temp = [1, 3]

i = 2
while len(temp) != N:
    temp.append(2*temp[i-2] + temp[i-1])
    i += 1

result = temp[len(temp)-1]
print(temp)
print(result % 10007)
